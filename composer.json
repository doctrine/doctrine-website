{
    "name": "doctrine/doctrine-website",
    "description": "Source code for doctrine-project.org",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com",
            "homepage": "https://jwage.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "algolia/algoliasearch-client-php": "^1.27",
        "cache/doctrine-adapter": "^1.0",
        "doctrine/inflector": "^1.4",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "doctrine/rst-parser": "^0.1",
        "doctrine/skeleton-mapper": "^1.0",
        "doctrine/static-website-generator": "^1.0",
        "erusev/parsedown": "^1.7",
        "knplabs/github-api": "^2.14",
        "pelago/emogrifier": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "scrivo/highlight.php": "^9.14",
        "sendgrid/sendgrid": "^7.3",
        "stripe/stripe-php": "^6.34",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/process": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "doctrine/coding-standard": "^10.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^1.1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpstan/phpstan-strict-rules": "^1.1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/css-selector": "^4.4",
        "symfony/dom-crawler": "^4.4",
        "symfony/error-handler": "^4.4",
        "symfony/var-dumper": "^4.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Website\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Website\\Tests\\": "tests/"
        }
    }
}
